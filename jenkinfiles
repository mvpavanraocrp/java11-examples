pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git 'https://github.com/hellokaton/java11-examples'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('publish') {
            steps {
                archiveArtifacts artifacts: 'target/*.jar'
            }
        }
    }
}
